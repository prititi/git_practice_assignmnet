function checkprime(num){
    let count=0
    for(let i=0; i<=num; i++){
        if(num%i===0){
            count++;
        }
    }
    if(count==2){
        return true;
    }
    return false;
    
}
let answer=Checkprime(13);
if(answer== true){
    console.log("Is a Prime Number")
}else{
    console.log("Not a Prime Number");
}